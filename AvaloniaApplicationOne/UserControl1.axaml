<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaApplicationOne.UserControl1" Background="Black">
	<Grid>
		<StackPanel Width="100">
			<TextBlock HorizontalAlignment="Center" Text="Username" Foreground="White"/>
			<TextBox Width="90" Height="20"></TextBox>
			<TextBlock HorizontalAlignment="Center" Text="Password" Foreground="White"/>
			<TextBox Width="90" Height="20"></TextBox>
		</StackPanel>
	</Grid>
</UserControl>
